{
    "name": "krendil/model-tree",
    "description": "Laravel Nova ModelTree tool. Simple package for manage tree models",
    "keywords": [
        "laravel",
        "nova",
        "tree"
    ],
    "authors": [
        {
            "name": "Serhii Goncharenko",
            "email": "krendil007@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Krendil\\ModelTree\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Krendil\\ModelTree\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
